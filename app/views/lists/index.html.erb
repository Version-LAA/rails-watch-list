<%= render "shared/navbar"%>
<%= render "banner"%>
<h1>Your List</h1>

<div class="cards">
  <%@lists.each do |list|%>
   <div class="card" style="width: 18rem;">
     <div class="card-body">
       <h5 class="card-title"><%=link_to list.name,list_path(list)%></h5>
       <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
     </div>
   </div>
  <%end%>
</div>
